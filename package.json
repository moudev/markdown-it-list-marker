  "license": "ISC",
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.2.0",
    "fs": "^0.0.1-security",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.7",
    "markdown-it": "^12.3.2",
    "os": "^0.1.2",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.70.1",
    "vitest": "^0.9.3",
    "yalc": "^1.0.0-pre.53"
